<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">


    <TextView
        android:id="@+id/notif_tv_ssg"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="166dp"
        android:fontFamily="@font/poppins"
        android:paddingLeft="13dp"
        android:text="Notification"
        android:textColor="#004AAD"
        android:textSize="30sp" />

    <View
        android:id="@+id/notif_ssg"
        android:layout_width="wrap_content"
        android:layout_height="582dp"
        android:layout_above="@+id/meeting_btn_notif_ssg"
        android:layout_below="@+id/notif_tv_ssg"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="23dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="25dp"
        android:background="@drawable/rounded_corner_view"
        android:backgroundTint="#EFEEFF" />

    <TextView
        android:id="@+id/notif_tv_1_ssg"
        android:layout_width="318dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/notif_ssg"
        android:layout_alignTop="@+id/notif_ssg"
        android:layout_alignEnd="@+id/notif_ssg"
        android:layout_marginStart="23dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="23dp"
        android:text=""
        android:textColor="#000000"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/notif_tv_2_ssg"
        android:layout_width="321dp"
        android:layout_height="50dp"
        android:layout_below="@+id/notif_tv_1_ssg"
        android:layout_alignStart="@+id/notif_tv_1_ssg"
        android:layout_alignEnd="@+id/notif_tv_1_ssg"
        android:layout_marginStart="0dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="1dp"
        android:text=""
        android:textColor="#000000"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/notif_tv_3_ssg"
        android:layout_width="321dp"
        android:layout_height="50dp"
        android:layout_below="@+id/notif_tv_2_ssg"
        android:layout_alignStart="@+id/notif_tv_2_ssg"
        android:layout_alignEnd="@+id/notif_tv_2_ssg"
        android:layout_marginStart="0dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="0dp"
        android:text=""
        android:textColor="#000000"
        android:textSize="20dp"/>

    <View
        android:id="@+id/notif_vw_ssg"
        android:layout_width="410dp"
        android:layout_height="2dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="54dp"
        android:background="#DBDEE3"/>

    <Button
        android:id="@+id/home_btn_notif_ssg"
        android:layout_width="47dp"
        android:layout_height="43dp"
        android:layout_alignTop="@+id/notif_vw_ssg"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="0dp"
        android:layout_marginBottom="5dp"
        android:background="@drawable/home_btn__1_" />

    <Button
        android:id="@+id/meeting_btn_notif_ssg"
        android:layout_width="52dp"
        android:layout_height="46dp"
        android:layout_alignTop="@+id/home_btn_notif_ssg"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="54dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:layout_toEndOf="@+id/home_btn_notif_ssg"
        android:background="@drawable/meeting_button" />

    <Button
        android:id="@+id/notif_btn_notif_ssg"
        android:layout_width="52dp"
        android:layout_height="46dp"
        android:layout_alignTop="@+id/meeting_btn_notif_ssg"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="54dp"
        android:layout_marginTop="0dp"
        android:layout_marginBottom="5dp"
        android:layout_toEndOf="@+id/meeting_btn_notif_ssg"
        android:background="@drawable/notif_btn_notif__1_" />

    <Button
        android:id="@+id/profile_btn_notif_ssg"
        android:layout_width="52dp"
        android:layout_height="46dp"
        android:layout_alignTop="@+id/notif_btn_notif_ssg"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="40dp"
        android:layout_marginTop="2dp"
        android:layout_marginBottom="3dp"
        android:layout_toEndOf="@+id/notif_btn_notif_ssg"
        android:background="@drawable/profile_button" />


</RelativeLayout>